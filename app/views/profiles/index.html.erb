<div class="flex justify-between">
  <%= render layout: 'shared/options' do %>
    <%= button_to t('create_new_profile'), new_profile_path, method: :get, class: 'text-gray-700 block px-4 py-2 text-sm', role:'menuitem', tabindex:'-1' %>
  <% end %>
  <%= render 'search', url: profiles_path%>
</div>
<ul class="mt-10">
  <% if @profiles.count.zero? %>
    <p> No Profiles found</p>
  <% else %>
    <% @profiles.each_with_index do |profile, index| %>
      <li>
        <%= render 'card', profile: profile %>
      </li>
      <% unless index + 1 == @profiles.count %>
        <div class="text-gray-300 flex justify-center cursor-default" onselectstart="return false">
          ________________
        </div>
      <% end %>
    <% end %>
  <% end %>
</ul>
